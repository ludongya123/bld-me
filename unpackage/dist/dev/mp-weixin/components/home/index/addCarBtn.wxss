.btn_add_car {
  width: 686rpx;
  height: 120rpx;
  background-color: #D6BFA9;
  margin: 0 auto 0;
  -webkit-border-radius: 60rpx;
          border-radius: 60rpx;
  position: absolute;
  z-index: 10;
  left: 32rpx;
  top: 408rpx;
  padding: 0 36rpx;
}
.btn_add_car.inner {
    top: -60rpx;
}
.btn_add_car.empty {
    background: #DECBB7;
}
.btn_add_car .empty-add-icon {
    width: 48rpx;
    height: 48rpx;
}
.btn_add_car .empty-add-text {
    color: #283540;
    margin-left: 20rpx;
}
.icon_car_img {
  width: 64rpx;
  height: 64rpx;
}
