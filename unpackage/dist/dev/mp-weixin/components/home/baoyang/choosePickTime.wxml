<view><block wx:if="{{chTimeFlage}}"><view data-event-opts="{{[['tap',[['closeModal',['$event']]]]]}}" class="modal_box" bindtap="__e"></view></block><view class="{{['pick_box',(chTimeFlage)?'show':'']}}"><view class="pick_top">{{''+(title||'选择到店时间')+''}}<image class="icon_delete" src="../../../static/home/index/icon_delete.png" data-event-opts="{{[['tap',[['closeModal',['$event']]]]]}}" bindtap="__e"></image></view><block wx:if="{{timeMsg.tips}}"><view class="discount_des">{{timeMsg.tips}}</view></block><view class="pick_content" scroll-y="true" height="688upx;"><view class="choose_date_box"><scroll-view class="choose_date_box_scroll" scroll-x="true" scroll-with-animation="true"><block wx:for="{{timeMsg.times}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['chooseDate',[index,'$0','$1','$2'],[[['timeMsg.times','',index,'date']],[['timeMsg.times','',index,'dateUse']],[['timeMsg.times','',index,'desc']]]]]]]}}" class="{{['scroll_item',(currentDate==index)?'active':'']}}" bindtap="__e"><view class="scroll_item_date">{{item.dateUse}}</view><view class="scroll_item_week">{{item.desc}}</view></view></block></scroll-view></view><view class="choose_time_box flex-start"><block wx:for="{{timeMsg.times[currentDate].times}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view data-event-opts="{{[['tap',[['chooseTime',[index,'$0'],[[['timeMsg.times.'+currentDate+'.times','',index,'time']]]]]]]}}" class="{{['time_item',(item.is_busy==1)?'is_busy':'']}}" bindtap="__e"><view class="{{['date-list',item.can_reserve==0||item.is_busy==1?'gray':'']}}">{{''+item.time}}<block wx:if="{{item.is_busy==1}}"><text class="no_loca">(已满)</text></block></view></view></block></block></view></view></view></view>