<view><block wx:if="{{storeFlag}}"><view data-event-opts="{{[['tap',[['checkStore',['close']]]]]}}" class="modal_box" bindtap="__e"></view></block><view class="{{['pick_box',(storeFlag)?'show':'']}}"><view class="pick_top">{{'选择'+tag+'4S店'}}<image class="icon_delete" src="../../../static/home/index/icon_delete.png" data-event-opts="{{[['tap',[['checkStore',['close']]]]]}}" bindtap="__e"></image></view><view class="pick_content" scroll-y="true" height="688upx;"><block wx:for="{{storeList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="pick_item_box"><view class="title_area">{{''+item.name+''}}</view><block wx:for="{{item.stores}}" wx:for-item="inItem" wx:for-index="inIndex" wx:key="inIndex"><block><view data-event-opts="{{[['tap',[['checkStore',['$0',index,inIndex],[[['storeList','',index],['stores','',inIndex]]]]]]]}}" class="pick_item" bindtap="__e"><image class="item_left_img" src="{{inItem.image}}" mode="aspectFill"></image><view class="car_msg"><view class="car_msg_title">{{''+inItem.name+''}}</view><view class="car_msg_des"><block wx:if="{{inItem.distance}}"><text>{{inItem.distance>=1?inItem.distance+'km':inItem.distance*1000+'m'}}</text></block>{{''+inItem.address+''}}</view></view><block wx:if="{{currentCheck==index+'-'+inIndex}}"><image class="icon_check_store" src="../../../static/home/baoyang/icon_check_store.png"></image></block></view><block wx:if="{{inIndex+1!=item.stores.length}}"><view class="xiline"></view></block></block></block></view></block></view></view></view>