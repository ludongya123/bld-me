<view><block wx:if="{{changeCarFlag}}"><view data-event-opts="{{[['tap',[['checkCar',['close']]]]]}}" class="modal_box" bindtap="__e"></view></block><view class="{{['pick_box',(changeCarFlag)?'show':'']}}"><view class="pick_top">选择车辆<image class="icon_delete" src="../../../static/home/index/icon_delete.png" data-event-opts="{{[['tap',[['checkCar',['close']]]]]}}" bindtap="__e"></image></view><view class="{{['pick_content',(yyCarList.length==5)?'pick_content_height':'']}}" scroll-y="true" height="688upx;"><block wx:for="{{yyCarList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['checkCar',['$0',index],[[['yyCarList','',index]]]]]]]}}" class="{{['pick_item',(currCarId==item.id)?'check_pick_item':'']}}" bindtap="__e"><image class="item_left_img" src="{{'https://carbrand.car-me.com/'+item.brand_id+'.png'}}"></image><view class="car_msg"><view class="car_msg_title">{{''+(item.car_no||'暂无车牌')+''}}</view><view class="car_msg_des">{{''+item.series_name+" "+item.vehicle_type_name+''}}</view></view><block wx:if="{{currCarId==item.id}}"><image class="icon_check_car" src="../../../static/home/baoyang/icon_check_car.png"></image></block></view></block></view><block wx:if="{{yyCarList.length<5}}"><view data-event-opts="{{[['tap',[['openAddCar']]]]}}" class="bottom_btn" bindtap="__e">添加车辆</view></block></view></view>