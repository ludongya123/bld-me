<view><block wx:if="{{isShow}}"><view data-event-opts="{{[['tap',[['closeModal',['$event']]]]]}}" class="modal_box" bindtap="__e"></view></block><view class="{{['pick_box',(isShow)?'show':'']}}"><view class="pick_top">{{''+(title||'选择到店时间')+''}}<image class="icon_delete" src="../../../static/home/index/icon_delete.png" data-event-opts="{{[['tap',[['closeModal',['$event']]]]]}}" bindtap="__e"></image></view><view class="pick_content" scroll-y="true" height="688upx;"><view class="choose_date_box"><scroll-view class="choose_date_box_scroll" scroll-x="true" scroll-with-animation="true"><block wx:for="{{times}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['chooseDate',[index,'$0','$1','$2'],[[['times','',index,'date']],[['times','',index,'dateUse']],[['times','',index,'desc']]]]]]]}}" class="{{['scroll_item',(currentDate==index)?'active':'']}}" bindtap="__e"><view class="scroll_item_date">{{item.dateUse||''}}</view><view class="scroll_item_week">{{item.desc||''}}</view></view></block></scroll-view></view><view class="choose_time_box flex-start"><block wx:for="{{times[currentDate].time_list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view data-event-opts="{{[['tap',[['chooseTime',[index,'$0'],[[['times.'+currentDate+'.time_list','',index]]]]]]]}}" class="time_item" bindtap="__e">{{''+item+''}}</view></block></block></view></view></view></view>