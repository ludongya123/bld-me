.temp1 {
  padding: 0 32rpx;
  -webkit-flex-wrap: wrap;
      -ms-flex-wrap: wrap;
          flex-wrap: wrap;
}
.temp1 .temp1-item {
    width: 333rpx;
    margin-left: 14rpx;
    margin-bottom: 14rpx;
}
.temp1 .temp1-item:nth-child(2n-1) {
      margin-left: 0;
}
.temp1 .temp1-item .img {
      width: 100%;
      display: block;
      height: 252rpx;
}
.temp1 .temp1-item .goods-info {
      background: #fff;
      padding: 16rpx 24rpx 32rpx;
}
.temp1 .temp1-item .goods-info .title {
        height: 72rpx;
        line-height: 36rpx;
        color: #333;
        line-height: 36rpx;
        font-size: 26rpx;
        overflow: hidden;
        text-overflow: ellipsis;
        display: -webkit-box;
        -webkit-line-clamp: 2;
        /*! autoprefixer: off */
        -webkit-box-orient: vertical;
        word-break: break-all;
        /* autoprefixer: on */
}
.temp1 .temp1-item .goods-info .price {
        color: #DC606C;
        font-size: 26rpx;
        margin-top: 16rpx;
        font-weight: 600;
}
.temp1 .temp1-item .goods-info .price text {
          font-size: 32rpx;
          font-weight: bold;
}
