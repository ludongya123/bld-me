<view><map style="width:100%;" id="myMap" longitude="{{markers[0].longitude}}" latitude="{{markers[0].latitude}}" scale="14" markers="{{markers}}" show-location data-event-opts="{{[['regionchange',[['mapChange',['$event']]]],['end',[['mapChangeEnd',['$event']]]]]}}" bindregionchange="__e" bindend="__e"><cover-image class="icon_move_location" src="../../../static/home/roadside/move_location.png" data-event-opts="{{[['tap',[['moveToLocation',['$event']]]]]}}" bindtap="__e"></cover-image><cover-view hidden="{{!(isLocation)}}" class="location_wrap"><cover-image class="icon_mark_box" src="../../../static/home/roadside/mark_box.png"></cover-image><cover-view class="location_wrap_text">{{locationName}}</cover-view></cover-view></map><view class="cover-view-box"><view class="map_des"><view class="map_des_address"><view class="map_des_address_title"><view class="des1">请确认救援地址</view><view class="des2">（可拖动地图定位）</view></view><input class="map_des_address_describe" type="text" data-event-opts="{{[['input',[['__set_model',['','locationName','$event',[]]]]]]}}" value="{{locationName}}" bindinput="__e"/></view><view class="xiline"></view><view data-event-opts="{{[['tap',[['yuyueStore',['$event']]]]]}}" class="map_des_store" bindtap="__e"><view class="shop_change_line_1 flex-start"><image class="icon_4s" src="../../../static/home/roadside/icon_res_store.png"></image><view>救援门店<text>（7*24小时）</text></view></view><view class="shop_change_line_2 flex-between"><view class="store-left"><view class="store-name">{{checkStoreMsg.name}}</view><view class="line_3_address flex-start"><view class="address">{{checkStoreMsg.address}}</view><block wx:if="{{checkStoreMsg.distance}}"><view class="flex-end"><image src="/static/home/roadside/icon_location.png" mode></image><text>{{checkStoreMsg.distance>=1?checkStoreMsg.distance+'km':checkStoreMsg.distance*1000+'m'}}</text></view></block></view></view><image class="icon_right" src="/static/user/a-l-l.png" mode></image></view></view></view><view class="person_info"><view class="info_item"><view class="info_item_left">联系手机号</view><input type="number" placeholder="请输入手机号" placeholder-style="color:#999" maxlength="11" data-event-opts="{{[['input',[['__set_model',['','tel','$event',[]]]]]]}}" value="{{tel}}" bindinput="__e"/></view><view class="xiline"></view><view class="info_item"><view class="info_item_left">是否需要拖车</view><picker class="info_item_right" value="{{railerIndex}}" range="{{arrayTrailer}}" data-event-opts="{{[['change',[['trailerPickerChange',['$event']]]]]}}" bindchange="__e">{{''+(arrayTrailer[railerIndex]||'不需要')+''}}</picker><image class="icon_right_arrow" src="/static/user/a-l-l.png"></image></view><view class="xiline"></view><view class="info_item"><view class="info_item_left">保险公司</view><picker class="info_item_right" value="{{compIndex}}" range="{{arrayBXComponeny}}" data-event-opts="{{[['change',[['CompChange',['$event']]]]]}}" bindchange="__e">{{''+(arrayBXComponeny[compIndex]||'请选择')+''}}</picker><image class="icon_right_arrow" src="/static/user/a-l-l.png"></image></view></view><view data-event-opts="{{[['tap',[['toCall',['$event']]]]]}}" class="btn_pay" bindtap="__e">发送定位并呼叫救援</view></view><y-y-pick-store vue-id="1" tag="救援" storeFlag="{{storeFlag}}" storeList="{{storeList}}" data-event-opts="{{[['^checkStore',[['checkStore']]]]}}" bind:checkStore="__e" bind:__l="__l"></y-y-pick-store></view>