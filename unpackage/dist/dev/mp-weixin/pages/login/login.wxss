.login_box {
  width: 100%;
  height: 100vh;
  background-color: #fff;
}
.login_box .img_logo {
    width: 136rpx;
    height: 136rpx;
    margin: 128rpx 0 0 308rpx;
}
.login_box .login_title {
    width: 100%;
    text-align: center;
    color: #333;
    font-size: 36rpx;
    font-weight: bold;
    margin-top: 32rpx;
}
.login_box .btn_wx_login {
    position: relative;
    width: 574rpx;
    height: 98rpx;
    color: #fff;
    font-size: 32rpx;
    font-weight: bold;
    background-color: #C9AD94;
    margin: 108rpx auto 0;
    padding-left: 40rpx;
}
.login_box .btn_wx_login.not_click {
      background-color: #ccc;
}
.login_box .btn_wx_login .icon_wx {
      position: absolute;
      width: 48rpx;
      height: 40rpx;
      left: 128rpx;
      top: 30rpx;
      z-index: 99;
}
.login_box .btn_wx_login .btn_title {
      line-height: 98rpx;
}
.login_box .btn_wx_login button {
      position: relative;
      display: block;
      width: 574rpx;
      margin-left: auto;
      margin-right: auto;
      padding-left: 14px;
      padding-right: 14px;
      -webkit-box-sizing: border-box;
              box-sizing: border-box;
      font-size: 18px;
      text-align: center;
      text-decoration: none;
      line-height: 2.55555556;
      -webkit-border-radius: 5px;
              border-radius: 5px;
      overflow: hidden;
      color: #fff;
      background-color: transparent;
      z-index: 999;
}
.login_box .btn_wx_login button::after {
      border: none;
}
.login_box .btn_ph_login {
    width: 574rpx;
    height: 98rpx;
    text-align: center;
    line-height: 98rpx;
    color: #C9AD94;
    font-size: 32rpx;
    font-weight: bold;
    background-color: #fff;
    border: 1rpx solid #C9AD94;
    margin: 48rpx auto;
}
.login_box .login_des {
    position: fixed;
    width: 100%;
    font-size: 24rpx;
    bottom: 56rpx;
}
.login_box .login_des .icon_check {
      width: 32rpx;
      height: 32rpx;
      padding: 14rpx;
}
.login_box .login_des .login_des_1 {
      color: #999;
}
.login_box .login_des .login_des_2 {
      color: #B49A83;
}
