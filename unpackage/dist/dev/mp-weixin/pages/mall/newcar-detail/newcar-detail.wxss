.handle-area {
  position: fixed;
  width: 100%;
  text-align: center;
  bottom: 0;
  height: 98rpx;
}
.handle-area .left {
    background: #fff;
    color: #B49A83;
    width: 286rpx;
    font-size: 32rpx;
    line-height: 98rpx;
}
