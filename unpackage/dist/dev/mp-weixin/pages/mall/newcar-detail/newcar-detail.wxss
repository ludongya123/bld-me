.layer {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, 0.5);
  z-index: 10;
}
.layer-content {
  position: fixed;
  width: 100%;
  z-index: 11;
  background: #fff;
  text-align: center;
  bottom: -480rpx;
  padding: 80rpx 32rpx 0;
  height: 468rpx;
  -webkit-transition: all 0.2s ease;
  -o-transition: all 0.2s ease;
  transition: all 0.2s ease;
}
.layer-content .icon {
    width: 32rpx;
    height: 32rpx;
    position: absolute;
    top: 40rpx;
    right: 40rpx;
}
.layer-content .title {
    color: #283540;
    font-size: 36rpx;
    font-weight: bold;
}
.layer-content .desc {
    color: #666;
    margin: 16rpx 0 60rpx;
}
.layer-content .input-tel {
    height: 68rpx;
}
.layer-content .btn {
    margin-top: 80rpx;
    line-height: 98rpx;
    background: #ccc;
    color: #fff;
    font-size: 32rpx;
    font-weight: bold;
    position: absolute;
    width: 100%;
    bottom: 0;
    left: 0;
}
.layer-content .btn.active {
      background: #C9AD94;
}
.layer-content.content2 {
    bottom: -580rpx;
    height: 566rpx;
}
.layer-content.content2 .flex-center {
      padding: 48rpx 0 20rpx;
}
.layer-content.content2 .flex-center text {
        color: #ccc;
}
.layer-content.content2 .flex-center text.active {
          color: #333;
}
.layer-content.content2 .flex-center image {
        width: 16rpx;
        height: 32rpx;
        margin-left: 16rpx;
}
.layer-content.show {
    bottom: 0;
}

