<view class="wrapper"><block wx:for="{{carList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['openCarDetail',['$0'],[[['carList','',index,'id']]]]]]]}}" class="item" bindtap="__e"><image src="{{item.image}}" mode="aspectFill" lazy-load></image><view class="car-info"><view class="car-title flex-between"><text class="car-name">{{item.name}}</text><text class="car-price">{{item.price_range}}</text></view><view hidden="{{!(item.represent)}}" class="intro">{{item.represent}}</view><view class="car-tags flex-start"><block wx:for="{{item.tags}}" wx:for-item="item_" wx:for-index="index_" wx:key="index_"><block><text>{{item_}}</text></block></block></view></view></view></block><load vue-id="1" show="{{isShowloading}}" bind:__l="__l"></load><block wx:if="{{scrollNodata}}"><view class="scroll-nodata">没有更多数据了~</view></block><block wx:if="{{carList.length==0}}"><view class="init-nodata">暂时没有相关车辆信息~</view></block></view>