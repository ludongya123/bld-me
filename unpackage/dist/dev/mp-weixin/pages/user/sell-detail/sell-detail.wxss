.desc-info {
  padding: 20rpx 32rpx;
  background: #fff;
}
.desc-info .flex-start {
    padding: 16rpx 0;
    -webkit-box-align: start;
    -webkit-align-items: flex-start;
        -ms-flex-align: start;
            align-items: flex-start;
}
.desc-info .flex-start text {
      color: #999;
      line-height: 1.3;
}
.desc-info .flex-start text.content {
        -webkit-box-flex: 1;
        -webkit-flex: 1;
            -ms-flex: 1;
                flex: 1;
        color: #444;
}
.service-detail, .money-package {
  background: #fff;
  padding: 0 32rpx 20rpx;
  margin-top: 16rpx;
}
.service-detail .title, .money-package .title {
    padding: 24rpx 0 32rpx;
    font-size: 32rpx;
    color: #333;
    font-weight: bold;
}
.service-detail .flex-start, .money-package .flex-start {
    margin-top: 40rpx;
    -webkit-box-align: start;
    -webkit-align-items: flex-start;
        -ms-flex-align: start;
            align-items: flex-start;
    color: #666;
}
.service-detail .flex-start .tag, .money-package .flex-start .tag {
      width: 62rpx;
      line-height: 28rpx;
      font-size: 20rpx;
      color: #fff;
      text-align: center;
      -webkit-border-radius: 4rpx;
              border-radius: 4rpx;
}
.service-detail .flex-start .tag.parts, .money-package .flex-start .tag.parts {
        background: -webkit-linear-gradient(315deg, #e4a796 0%, #af6744 100%);
        background: -o-linear-gradient(315deg, #e4a796 0%, #af6744 100%);
        background: linear-gradient(135deg, #e4a796 0%, #af6744 100%);
}
.service-detail .flex-start .tag.man-hour, .money-package .flex-start .tag.man-hour {
        background: -webkit-linear-gradient(315deg, #b4c6cd 0%, #889cb6 100%);
        background: -o-linear-gradient(315deg, #b4c6cd 0%, #889cb6 100%);
        background: linear-gradient(135deg, #b4c6cd 0%, #889cb6 100%);
}
.service-detail .flex-start .desc_, .money-package .flex-start .desc_ {
      line-height: 36rpx;
      margin: 0  120rpx 0 14rpx;
      -webkit-box-flex: 1;
      -webkit-flex: 1;
          -ms-flex: 1;
              flex: 1;
      position: relative;
      top: -6rpx;
}
.service-detail .flex-start .desc_ {
  top: -8rpx;
}
.money-package .flex-start .tag {
  width: 96rpx;
  line-height: 28rpx;
  font-size: 24rpx;
  color: #B87252;
  border: 1rpx solid #B87252;
}
.money-package .flex-start .count {
  color: #B87252;
}
.money-package .flex-start.money .tag {
  color: #666;
  border: 1rpx solid #666;
  background: #f2f2f2;
  letter-spacing: 4rpx;
}
.btn {
  text-align: center;
  background: #fff;
  padding: 50rpx 0;
}
.btn button {
    width: 414rpx;
    line-height: 84rpx;
    height: 84rpx;
    -webkit-border-radius: 0;
            border-radius: 0;
    color: #B49A83;
    background: #fff;
}
.btn button:after {
      border: 1rpx solid #B49A83;
      -webkit-border-radius: 0;
              border-radius: 0;
}
.btn button.button-hover {
      color: #B49A83;
      background: #f1f1f1;
}
.btn button image {
      width: 48rpx;
      height: 48rpx;
      margin-right: 10rpx;
}

