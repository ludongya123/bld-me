.header-state {
  background: #fff;
  text-align: center;
  padding: 72rpx 0 80rpx;
}
.header-state image {
    width: 88rpx;
    height: 88rpx;
}
.header-state .s-text {
    color: #283540;
    font-size: 36rpx;
    margin: 16rpx 0 40rpx;
}
.header-state button {
    background: #fff;
    width: 170rpx;
    color: #999;
    line-height: 48rpx;
    font-size: 24rpx;
    -webkit-border-radius: 24rpx;
            border-radius: 24rpx;
    border: 1rpx solid #dedede;
}
.header-state button:after {
      -webkit-border-radius: 0;
              border-radius: 0;
      border: none;
}
.header-state button.button-hover {
      background: #fafafa;
}
.menu-list {
  margin: 16rpx 0;
  background: #fff;
  padding: 0 32rpx;
}
.menu-list .flex-between {
    padding: 26rpx 0;
}
.menu-list .flex-between .left {
      width: 200rpx;
}
.menu-list .flex-between .left .icon {
        width: 32rpx;
        height: 32rpx;
        vertical-align: middle;
}
.menu-list .flex-between .left .title {
        margin-left: 16rpx;
        color: #666;
        line-height: 40rpx;
        font-size: 28rpx;
}
.menu-list .flex-between .right {
      color: #283540;
      line-height: 40rpx;
      font-size: 28rpx;
      font-weight: bold;
      -webkit-box-flex: 1;
      -webkit-flex: 1;
          -ms-flex: 1;
              flex: 1;
      text-align: right;
}
.menu-list .flex-between .flex-end {
      color: #B49A83;
      line-height: 40rpx;
      font-size: 28rpx;
}
.menu-list .flex-between .flex-end text {
        font-weight: bold;
}
.menu-list .flex-between .flex-end image {
        width: 28rpx;
        height: 28rpx;
}
.order-no {
  padding: 16rpx 32rpx;
  background: #fff;
}
.order-no view {
    color: #999;
    line-height: 44rpx;
    font-size: 26rpx;
}

