page {
  background-color: #F3F5F8;
}
.logistics-Information .flex-start-top {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: space-start;
  -webkit-justify-content: space-start;
      -ms-flex-pack: space-start;
          justify-content: space-start;
  webkit-box-align: flex-start;
  -webkit-align-items: flex-start;
  -ms-flex-align: flex-start;
  -webkit-box-align: start;
          align-items: flex-start;
}
.logistics-Information .paddingbt-32 {
  padding-bottom: 32rpx;
}
.logistics-Information .logistics-top {
  position: relative;
}
.logistics-Information .logistics-top .logistics-top-bg {
    position: absolute;
    width: 100%;
    left: 0;
    top: 0;
    height: 96rpx;
    background: #283540;
}
.logistics-Information .logistics-top .logistics-top-box {
    padding: 24rpx 32rpx;
    position: relative;
    z-index: 1;
}
.logistics-Information .logistics-top .logistics-top-box .logistics-top-cont {
      background: #FFFFFF;
      -webkit-border-radius: 8rpx;
              border-radius: 8rpx;
      padding: 24rpx 30rpx;
}
.logistics-Information .logistics-top .logistics-top-box .logistics-top-cont .logistics-top-img {
        width: 96rpx;
        height: 96rpx;
}
.logistics-Information .logistics-top .logistics-top-box .logistics-top-cont .logistics-top-info {
        padding-left: 24rpx;
}
.logistics-Information .logistics-top .logistics-top-box .logistics-top-cont .logistics-top-info text {
          display: block;
          color: #666666;
          font-size: 28rpx;
}
.logistics-Information .logistics-ft {
  padding: 32rpx;
}
.logistics-Information .logistics-ft .logistics-ft-box {
    padding: 56rpx 40rpx;
    background: #FFFFFF;
    -webkit-border-radius: 8rpx;
            border-radius: 8rpx;
}
.logistics-Information .logistics-ft .logistics-ft-box .logistics-detail {
      position: relative;
      border-left: 2rpx solid #bdbdbd;
      padding: 0rpx 0rpx 30rpx 25px;
}
.logistics-Information .logistics-ft .logistics-ft-box .logistics-detail .logistics-detail-content text {
        display: block;
        padding-bottom: 10rpx;
        color: #999999;
        font-size: 28rpx;
}
.logistics-Information .logistics-ft .logistics-ft-box .logistics-detail .logistics-detail-sign {
        position: absolute;
        display: inline-block;
        left: -11rpx;
        top: 0rpx;
        width: 26rpx;
        height: 26rpx;
        -webkit-border-radius: 50%;
        background: #BDBDBD;
}
.logistics-Information .logistics-ft .logistics-ft-box .logistics-detail:first-child .logistics-detail-content text {
      color: #C76B41;
      font-weight: bold;
}
.logistics-Information .logistics-ft .logistics-ft-box .logistics-detail:last-child {
      border-left: none;
      padding-bottom: 0;
}
.logistics-Information .logistics-ft .logistics-ft-box .logistics-detail:last-child .logistics-detail-content text {
        display: block;
        padding-bottom: 10rpx;
        color: #999999;
        font-size: 28rpx;
        font-weight: normal;
}

