page {
  page-top: 16rpx;
}
.s-list {
  padding: 0 32rpx 48rpx;
  background: #fff;
  margin-bottom: 16rpx;
}
.s-list .title {
    padding: 32rpx 0;
}
.s-list .title .title-left {
      font-size: 32rpx;
      color: #333;
      font-weight: bold;
      -webkit-box-flex: 1;
      -webkit-flex: 1;
          -ms-flex: 1;
              flex: 1;
      line-height: 1.3;
}
.s-list .title .state {
      color: #999;
      margin-left: 20rpx;
}
.s-list .title .state.wait {
        color: #EA6572;
}
.s-list .tag {
    padding: 24rpx 0;
    color: #999;
    font-size: 26rpx;
}
.s-list .tag text {
      margin-right: 80rpx;
}
.s-list .content {
    line-height: 40rpx;
    color: #666;
    word-break: break-all;
}
.s-list .img-wrapper {
    -webkit-flex-wrap: wrap;
        -ms-flex-wrap: wrap;
            flex-wrap: wrap;
}
.s-list .img-wrapper image {
      width: 216rpx;
      height: 172rpx;
      margin-left: 18rpx;
      margin-top: 20rpx;
}
.s-list .img-wrapper image:nth-child(3n-2) {
        margin-left: 0;
}
.s-list .handel-area {
    padding-top: 48rpx;
    color: #666;
}
.s-list .handel-area .flex-start {
      margin-left: 60rpx;
}
.s-list .handel-area image {
      width: 36rpx;
      height: 36rpx;
      margin-right: 10rpx;
}
.reply {
  padding: 32rpx;
  background: #F3F5F8;
  margin-top: 32rpx;
}
.reply .reply-title {
    color: #666;
    font-weight: bold;
    margin-bottom: 18rpx;
}
.reply .reply-content {
    color: #999;
    line-height: 40rpx;
    font-size: 26rpx;
    word-break: break-all;
}
.nodata {
  padding: 30rpx;
  text-align: center;
  color: #666;
  font-size: 24rpx;
}
.no-init-data {
  text-align: center;
  padding-top: 200rpx;
  color: #B7B7B7;
}
.no-init-data image {
    width: 100%;
    height: 232rpx;
}
.no-init-data view {
    font-size: 32rpx;
}

