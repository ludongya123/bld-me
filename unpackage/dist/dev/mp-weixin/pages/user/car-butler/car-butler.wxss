.top {
  padding: 32rpx;
  height: 220rpx;
  background: #283540;
  position: relative;
}
.top .car-info image {
    width: 72rpx;
    height: 72rpx;
    margin-right: 24rpx;
}
.top .car-info .title-name {
    font-size: 36rpx;
    color: #fff;
    font-weight: bold;
}
.top .car-info .title-desc {
    font-size: 24rpx;
    color: #767F86;
    margin-top: 16rpx;
}
.top .menu {
    padding: 28rpx 0 24rpx;
    position: absolute;
    width: 686rpx;
    left: 32rpx;
    bottom: -100rpx;
    background: #fff;
    text-align: center;
}
.top .menu .icon {
      width: 64rpx;
      height: 64rpx;
}
.top .menu .menu-list-tip {
      color: #B49A83;
      line-height: 34rpx;
      font-size: 24rpx;
      font-weight: bold;
      margin-top: 16rpx;
}
.notice-list {
  width: 686rpx;
  margin: 0 auto;
  background: #fff;
  margin-top: 125rpx;
}
.notice-list .notice-title {
    padding: 32rpx 32rpx 6rpx;
}
.notice-list .notice-title .flex-start {
      font-size: 32rpx;
      font-weight: bold;
      color: #333;
}
.notice-list .notice-title .flex-start image {
        width: 36rpx;
        height: 36rpx;
        margin-right: 16rpx;
}
.notice-list .notice-title .flex-end {
      font-size: 26rpx;
      color: #999;
}
.notice-list .notice-title .flex-end image {
        width: 16rpx;
        height: 32rpx;
        margin-left: 16rpx;
}
.notice-list .list-g .list-i {
    padding: 26rpx 32rpx;
}
.notice-list .list-g .list-i image {
      width: 36rpx;
      height: 36rpx;
}
.notice-list .list-g .list-i .notice-desc {
      color: #B49A83;
      -webkit-box-flex: 1;
      -webkit-flex: 1;
          -ms-flex: 1;
              flex: 1;
      margin: 0 16rpx;
      line-height: 1.3;
}
.notice-list .list-g .list-i .notice-time {
      color: #999;
      font-size: 26rpx;
}
.notice-list .flex-center {
    padding: 32rpx 0;
    color: #999;
}
.notice-list .flex-center image {
      width: 34rpx;
      height: 16rpx;
}
.notice-list .not-open {
    padding: 52rpx 32rpx 60rpx;
}
.notice-list .not-open .f-l {
      -webkit-box-flex: 1;
      -webkit-flex: 1;
          -ms-flex: 1;
              flex: 1;
      margin-right: 20rpx;
}
.notice-list .not-open .f-l .tip {
        color: #666;
        line-height: 1.5;
}
.notice-list .not-open .f-l .btn {
        text-align: center;
        border: 1rpx solid #B49A83;
        width: 180rpx;
        padding: 10rpx 0;
        color: #B49A83;
        margin-top: 20rpx;
}
.notice-list .not-open .f-l .btn image {
          width: 32rpx;
          height: 32rpx;
          vertical-align: middle;
}
.notice-list .not-open .wkt {
      width: 144rpx;
      height: 144rpx;
}
.notice-list .no-notice {
    color: #666;
    padding: 52rpx 32rpx 62rpx;
}
.car-location {
  background: #fff;
  width: 686rpx;
  margin: 24rpx auto 0;
  margin-bottom: 30rpx;
}
.car-location .local-title {
    padding: 32rpx 32rpx 0;
    font-size: 32rpx;
    color: #333;
    font-weight: bold;
}
.car-location .local-title image {
      width: 36rpx;
      height: 36rpx;
      margin-right: 16rpx;
}
.car-location .addr-d {
    color: #999;
    font-size: 26rpx;
    margin: 16rpx 0 24rpx;
    padding: 0 32rpx;
}
.car-location .map {
    width: 686rpx;
    height: 500rpx;
}
.car-location .no-location {
    text-align: center;
    line-height: 20vh;
    background: #f0f0f0;
    color: #666;
}

