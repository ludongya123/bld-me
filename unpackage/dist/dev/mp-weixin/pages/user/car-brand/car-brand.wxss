page {
  background: #fff;
}
.keys {
  position: fixed;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
      -ms-flex-direction: column;
          flex-direction: column;
  padding: 60rpx 0;
  height: 100vh;
  background: rgba(222, 222, 222, 0.2);
  z-index: 10;
  right: 0;
  font-size: 32rpx;
  font-weight: bold;
}
.keys text {
    width: 60rpx;
    text-align: center;
    line-height: 40rpx;
}
.title {
  padding: 16rpx 32rpx;
  font-size: 30rpx;
  font-weight: bold;
  color: #333;
  background: #F1F1F1;
}
.car-brand .flex-start {
  border-top: 1rpx solid transparent;
  -webkit-flex-wrap: wrap;
      -ms-flex-wrap: wrap;
          flex-wrap: wrap;
  padding-bottom: 60rpx;
}
.car-brand .flex-start .h-b-list {
    width: 20%;
    text-align: center;
    margin-top: 60rpx;
}
.car-brand .flex-start .h-b-list image {
      width: 80rpx;
      height: 80rpx;
}
.car-brand .flex-start .h-b-list .car-brand-name {
      color: #555;
      margin-top: 20rpx;
}
.brand-list .flex-start {
  padding: 20rpx 40rpx;
  color: #555;
}
.brand-list .flex-start image {
    width: 80rpx;
    height: 80rpx;
    margin-right: 20rpx;
}
scroll-view {
  height: 100vh;
}

