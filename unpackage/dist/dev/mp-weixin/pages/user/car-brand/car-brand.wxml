<view class="wrapper"><view class="keys flex-between"><block wx:for="{{keys}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><text data-event-opts="{{[['tap',[['jumpIndex',['$0'],[[['keys','',index]]]]]]]}}" bindtap="__e">{{item}}</text></block></block></view><scroll-view class="brand-list" scroll-y scroll-into-view="{{curView}}" scroll-with-animation><view class="car-brand"><view class="title">热门品牌</view><view class="flex-start"><block wx:for="{{hotBrandList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view data-event-opts="{{[['tap',[['openBrandStyle',['$0','$1'],[[['hotBrandList','',index,'id']],[['hotBrandList','',index,'name']]]]]]]}}" class="h-b-list" bindtap="__e"><image src="{{carIcon+item.id+'.png'}}" mode="aspectFill" lazy-load></image><view class="car-brand-name">{{item.name}}</view></view></block></block></view></view><block wx:for="{{brandList}}" wx:for-item="item" wx:for-index="key" wx:key="key"><block><view><view class="title" id="{{'car'+item.key}}">{{item.key}}</view><block wx:for="{{item.brandList}}" wx:for-item="itm" wx:for-index="index" wx:key="index"><block><view><view data-event-opts="{{[['tap',[['openBrandStyle',['$0','$1'],[[['brandList','',key],['brandList','',index,'id']],[['brandList','',key],['brandList','',index,'name']]]]]]]}}" class="flex-start" bindtap="__e"><image src="{{carIcon+itm.id+'.png'}}" mode="aspectFill"></image><text>{{itm.name}}</text></view><view class="xiline"></view></view></block></block></view></block></block></scroll-view><car-style vue-id="1" brandId="{{brandId}}" show="{{showBrandStyle}}" data-event-opts="{{[['^hide',[['hideCarStyle']]]]}}" bind:hide="__e" bind:__l="__l"></car-style></view>