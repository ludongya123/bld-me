page {
  page-top: 16rpx;
}
.my-order {
  padding: 0 34rpx 48rpx;
  background: #fff;
  margin: 16rpx 0;
}
.my-order .order-top {
    padding: 22rpx 0;
    padding-top: 34rpx;
}
.my-order .order-top .order-top-left, .my-order .order-top .order-top-right {
      color: #999999;
      font-size: 28rpx;
}
.my-order .flex-start-order {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: space-start;
    -webkit-justify-content: space-start;
        -ms-flex-pack: space-start;
            justify-content: space-start;
    webkit-box-align: flex-start;
    -webkit-align-items: flex-start;
    -ms-flex-align: flex-start;
    -webkit-box-align: start;
            align-items: flex-start;
}
.my-order .flex-start-order .flex-content-order {
      -webkit-box-flex: 1;
      -webkit-flex: 1;
          -ms-flex: 1;
              flex: 1;
}
.my-order .order-content {
    padding: 24rpx 0;
}
.my-order .order-content .order-img {
      width: 192rpx;
      height: 142rpx;
}
.my-order .order-content .order-describe {
      width: 326rpx;
      padding-left: 24rpx;
}
.my-order .order-content .order-describe .order-title {
        font-size: 28rpx;
        line-height: 34rpx;
        overflow: hidden;
        -o-text-overflow: ellipsis;
           text-overflow: ellipsis;
        display: -webkit-box;
        -webkit-line-clamp: 3;
        -webkit-box-orient: vertical;
}
.my-order .order-content .order-describe .order-specifications {
        color: #999999;
        font-size: 24rpx;
        line-height: 30rpx;
        margin-top: 10rpx;
        overflow: hidden;
        -o-text-overflow: ellipsis;
           text-overflow: ellipsis;
        display: -webkit-box;
        -webkit-line-clamp: 2;
        -webkit-box-orient: vertical;
}
.my-order .order-amounta {
    color: #666666;
    font-size: 28rpx;
    padding: 24rpx 0 20rpx 0;
    padding-bottom: 0;
}
.my-order .order-payment-amounta {
    color: #666666;
    font-size: 28rpx;
    padding-top: 20rpx;
}
.my-order .order-payment-amounta text {
      font-size: 32rpx;
      color: #EA6572;
}
.my-order .order-info {
    width: 200rpx;
}
.my-order .order-info text {
      display: block;
      text-align: right;
}
.my-order .order-info .final-price {
      color: #333333;
      font-size: 28rpx;
      margin-bottom: 10rpx;
}
.my-order .order-info .total-price {
      color: #999999;
      font-size: 28rpx;
      text-decoration: line-through;
}
.my-order .order-info .commodity-number {
      color: #333333;
      font-size: 26rpx;
      margin-top: 32rpx;
}
.my-order .flex-column-order {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
        -ms-flex-direction: column;
            flex-direction: column;
    -webkit-box-align: center;
    -webkit-align-items: center;
        -ms-flex-align: center;
            align-items: center;
    text-align: right;
}
.reply {
  padding: 32rpx;
  background: #F3F5F8;
  margin-top: 32rpx;
}
.reply .reply-title {
    color: #666;
    font-weight: 600;
    margin-bottom: 18rpx;
}
.reply .reply-content {
    color: #999;
    line-height: 40rpx;
    font-size: 26rpx;
    word-break: break-all;
}
.nodata {
  padding: 30rpx;
  text-align: center;
  color: #666;
  font-size: 24rpx;
}
.no-init-data {
  padding-top: 248rpx;
  text-align: center;
  color: #B7B7B7;
}
.no-init-data image {
    width: 100%;
    height: 232rpx;
}

