<view class="wrapper"><view class="header"><view class="bg"></view><view class="bg bg1"></view><view class="swiper-container"><swiper class="swiper" previous-margin="14px" next-margin="14px" current="{{currIndex}}" indicator-dots="{{false}}" interval="{{3000}}" duration="{{1000}}" indicator-active-color="#2A3440" data-event-opts="{{[['change',[['changeSwiper',['$event']]]]]}}" bindchange="__e"><block wx:for="{{carList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><view class="swiper-item"><view data-event-opts="{{[['tap',[['openEditCar',['$0','$1','$2','$3'],[[['carList','',index,'id']],[['carList','',index,'is_ico']],[['carList','',index,'car_no']],[['carList','',index,'brand_id']]]]]]]}}" class="car-info flex-start" bindtap="__e"><image class="img" src="{{carIcon+item.brand_id+'.png'}}" mode></image><view class="car-detail"><view class="flex-between car-name"><view class="car-code flex-start"><text>{{item.car_no||'暂无车牌'}}</text><image hidden="{{!(item.is_ico==1)}}" src="/static/user/v.png" mode></image></view><view class="edit flex-end"><image class="icon" src="/static/user/edit.png" mode></image><text>编辑</text></view></view><view class="car-subname">{{''+item.series_name+" "+item.vehicle_type_name+''}}</view><view class="car-mileage">{{'行驶里程：'+(item.mileage?item.mileage+'km':'--')+''}}</view></view></view><view class="xiline"></view><view class="flex-between handel-area"><block wx:if="{{item.car_no}}"><view data-event-opts="{{[['tap',[['setDefaultCarHandel',['$0',index],[[['carList','',index,'id']]]]]]]}}" class="set-default flex-start" bindtap="__e"><block wx:if="{{item.is_default==1}}"><image class="icon" src="/static/user/secd.png" mode></image></block><block wx:if="{{item.is_default!=1}}"><image class="icon" src="/static/user/circle.png" mode></image></block><text>{{item.is_default==1?'默认车辆':'设为默认车辆'}}</text></view></block><block wx:if="{{!item.car_no&&item.is_obd!=1&&item.is_ico==1}}"><view class="tip">请完善车辆信息</view></block><block wx:else><view></view></block><view class="del-car flex-start" data-id="{{item.id}}" data-event-opts="{{[['tap',[['delCarHandel',['$0',index],[[['carList','',index,'id']]]]]]]}}" bindtap="__e"><block wx:if="{{item.is_obd==1}}"><block><image class="icon" src="/static/user/gj.png" mode></image><text class="del-text">爱车管家</text></block></block><block wx:if="{{item.is_obd!=1&&item.is_ico!=1}}"><block><image class="icon" src="/static/user/del.png" mode></image><text class="del-text">删除车辆</text></block></block></view></view></view></swiper-item></block><block wx:if="{{carId==''}}"><swiper-item><view data-event-opts="{{[['tap',[['openEditMore']]]]}}" class="swiper-item add" bindtap="__e"><block wx:if="{{totalCar<5}}"><image class="add-icon" src="/static/user/add-car.png" mode></image></block><block wx:if="{{totalCar>=5}}"><image class="add-icon" src="/static/user/more-car.png" mode></image></block><block wx:if="{{totalCar<5}}"><view class="add-tip">点击添加车辆</view></block><block wx:if="{{totalCar>=5}}"><view class="add-tip">点击查看更多车辆</view></block></view></swiper-item></block></swiper></view></view><view class="remind"><view class="title">信息提醒</view><view class="xiline"></view><view class="menu-list"><view class="flex-start"><image class="icon" src="/static/user/lc.png" mode></image><text class="menu-title">下次保养里程（km）</text><text class="menu-value">{{afterSaleINfo.car_info.next_maintain_mileage?afterSaleINfo.car_info.next_maintain_mileage/1000:'--'}}</text></view><view class="flex-start"><image class="icon" src="/static/user/cx.png" mode></image><text class="menu-title">保险到期时间</text><text class="menu-value">{{afterSaleINfo.car_info.insurance_expiration_time||'--'}}</text></view><view class="flex-start"><image class="icon" src="/static/user/nj.png" mode></image><text class="menu-title">年检到期时间</text><text class="menu-value">{{afterSaleINfo.car_info.inspection_expiration_time||'--'}}</text></view></view></view><view class="gap"></view><view class="service-recode"><view class="title">售后服务记录</view><view class="xiline"></view><view class="s-group"><block wx:if="{{afterSaleINfo.records.length==0}}"><view class="nodata">没有服务记录~</view></block><block wx:for="{{afterSaleINfo.records}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view data-event-opts="{{[['tap',[['openPage',['sell-detail','$0'],[[['afterSaleINfo.records','',index,'id']]]]]]]}}" class="s-item flex-between" bindtap="__e"><view class="s-item-l"><view class="flex-start"><text>工单编号：</text><text class="content">{{item.work_order}}</text></view><view class="flex-start"><text>服务时间：</text><text class="content">{{item.time}}</text></view><view class="flex-start"><text>服务门店：</text><text class="content">{{item.store_name}}</text></view><view class="flex-start"><text>服务顾问：</text><text class="content">{{item.adviser}}</text></view></view><view class="s-item-r"><image class="icon" src="/static/user/a-l-d.png" mode></image></view></view></block></block></view></view></view>