page {
  background-color: #ffffff;
}
.user-switching-accounts {
  width: 100%;
  background-color: #fff;
}
.user-switching-accounts .user-switching-tips {
    background: #889CB6;
    color: #FFFFFF;
    font-size: 24rpx;
    padding: 20rpx 32rpx;
}
.user-switching-accounts .user-switching-content {
    padding: 0 32rpx;
}
.user-switching-accounts .user-switching-content .user-switching-top {
      background: #fff;
      padding: 48rpx 0;
}
.user-switching-accounts .user-switching-content .user-switching-top .user-switching-top-text {
        font-size: 32rpx;
        color: #4A4A4A;
        font-weight: bold;
}
.user-switching-accounts .user-switching-content .user-switching-top text {
        color: #666666;
        font-size: 32rpx;
        padding-left: 32rpx;
        text-align: left;
}
.user-switching-accounts .flex-selec-start {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: space-start;
    -webkit-justify-content: space-start;
        -ms-flex-pack: space-start;
            justify-content: space-start;
    webkit-box-align: flex-start;
    -webkit-align-items: flex-start;
    -ms-flex-align: flex-start;
    -webkit-box-align: start;
            align-items: flex-start;
}
.user-switching-accounts .user-accounts-select {
    background: #fff;
    margin: 0 auto 0;
    width: 686rpx;
    position: relative;
}
.user-switching-accounts .user-accounts-select .user-accounts-select-left {
      width: 264rpx;
      height: 120rpx;
      line-height: 120rpx;
      font-size: 32rpx;
      color: #4A4A4A;
      font-weight: bold;
}
.user-switching-accounts .user-accounts-select .user-accounts-select-right {
      -webkit-box-flex: 1;
      -webkit-flex: 1;
          -ms-flex: 1;
              flex: 1;
}
.user-switching-accounts .user-accounts-select .user-accounts-select-right .item-select {
        height: 120rpx;
        line-height: 120rpx;
}
.user-switching-accounts .user-accounts-select .user-accounts-select-right .item-select text {
          font-size: 32rpx;
          color: #666666;
}
.user-switching-accounts .user-accounts-select .user-accounts-select-right .item-select .sleect-text {
          color: #B49A83;
}
.user-switching-accounts .user-accounts-select .icon-box {
      -webkit-box-flex: 1;
      -webkit-flex: 1;
          -ms-flex: 1;
              flex: 1;
      text-align: right;
}
.user-switching-accounts .user-accounts-select .icon-box .icon {
        width: 48rpx;
        height: 48rpx;
        display: inline-block;
        margin-left: 20rpx;
}
.user-switching-accounts .flex-start .icon {
    width: 48rpx;
    height: 48rpx;
    display: block;
    margin-left: 20rpx;
}
.user-switching-accounts .flex-start .a-icon {
    width: 16rpx;
    height: 32rpx;
}
.user-switching-accounts .submit {
    color: #fff;
    line-height: 100rpx;
    font-size: 32rpx;
    margin: 0 88rpx;
    margin-top: 50rpx;
    font-weight: 500;
    background: #CCCCCC;
    text-align: center;
}
.user-switching-accounts .submit.active {
      background: #C9AD94;
}
.user-switching-accounts .submit.btn-bg-none {
      background: none;
      color: #C9AD94;
      border: 2rpx solid #C9AD94;
}
