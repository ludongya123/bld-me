<view class="wrapper"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="swiper-item"><view data-event-opts="{{[['tap',[['openMyCar',['$0'],[[['list','',index,'id']]]]]]]}}" class="car-info flex-start" bindtap="__e"><image class="img" src="{{carIcon+item.brand_id+'.png'}}" mode></image><view class="car-detail"><view class="car-name flex-start"><text>{{item.car_no||'暂无车牌'}}</text><image hidden="{{!(item.is_ico==1)}}" src="/static/user/v.png" mode></image></view><view class="car-subname">{{''+item.series_name+" "+item.vehicle_type_name+''}}</view><view class="car-mileage">{{'行驶里程：'+(item.mileage?item.mileage+"km":'--')+''}}</view></view></view><view class="xiline"></view><view class="flex-between handel-area"><block wx:if="{{item.car_no}}"><view data-event-opts="{{[['tap',[['setDefaultCarHandel',['$0',index],[[['list','',index,'id']]]]]]]}}" class="set-default flex-start" bindtap="__e"><block wx:if="{{item.is_default==1}}"><image class="icon" src="/static/user/secd.png" mode></image></block><block wx:if="{{item.is_default!=1}}"><image class="icon" src="/static/user/circle.png" mode></image></block><text>{{item.is_default==1?'默认车辆':'设置默认车辆'}}</text></view></block><block wx:else><view></view></block><view class="edit-del flex-start"><view data-event-opts="{{[['tap',[['edit',['$0','$1','$2','$3'],[[['list','',index,'id']],[['list','',index,'is_ico']],[['list','',index,'car_no']],[['list','',index,'brand_id']]]]]]]}}" class="edit flex-end" bindtap="__e"><image class="icon" src="/static/user/edit.png" mode></image><text>编辑</text></view><block wx:if="{{item.is_obd!=1&&item.is_ico!=1}}"><view data-event-opts="{{[['tap',[['delCarH',['$0',index],[[['list','',index,'id']]]]]]]}}" class="del flex-end" bindtap="__e"><image class="icon" src="/static/user/del.png" mode></image><text class="del-text">删除车辆</text></view></block><block wx:if="{{item.is_obd==1}}"><view data-event-opts="{{[['tap',[['openObd',['$0'],[[['list','',index,'id']]]]]]]}}" class="del flex-end" bindtap="__e"><image class="icon" src="/static/user/gjh.png" mode></image><text class="del-text">爱车管家</text></view></block></view></view></view></block><load vue-id="1" show="{{isShowLoading}}" bind:__l="__l"></load><block wx:if="{{scrollNodata}}"><view class="nodata">没有更多数据了~</view></block></view>